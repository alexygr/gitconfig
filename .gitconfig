[user]
    name = algrin
    email = alexy.grinman@outlook.com
[core]
    autocrlf = true
    safecrlf = warn
    quotpath = off
    editor = nvim-qt.exe
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = red bold
    untracked = cyan
[alias]
    ch = checkout
    co = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
